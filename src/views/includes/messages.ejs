<% if (msgErrors.length > 0) { %>
<div class="row">
	<div class="col my-3">
		<div class="alert alert-danger">
            <% msgErrors.forEach(erro => {%>
                <%= erro %> <br>
            <% });%>
        </div>      
	</div>
</div>
<% } %>

<% if (msgSuccess.length > 0) { %>
<div class="row">
	<div class="col my-3">
		<div class="alert alert-success">
            <% msgSuccess.forEach(success => {%>
                <%= success %> <br>
            <% });%>
        </div>      
	</div>
</div>
<% } %>

